{
	"appId": "com.tingreader.app",
	"appName": "Ting Reader",
	"webDir": "dist",
	"server": {
		"androidScheme": "https",
		"allowNavigation": [
			"*"
		]
	},
	"plugins": {
		"CapacitorHttp": {
			"enabled": true
		},
		"SplashScreen": {
			"launchShowDuration": 0,
			"launchAutoHide": false,
			"backgroundColor": "#ffffffff",
			"androidSplashResourceName": "splash",
			"showSpinner": false
		}
	}
}
